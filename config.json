{
  "server": {
    "port": "600",
    "dir": "www/",
    "ssl": {
      "key": "ssl/captn.key",
      "cert": "ssl/captn.crt"
  },
  "page": {
    "default": "index.html",
    "script": "node.js"
  },
  "methods": {
    "allowed": ["OPTIONS", "GET", "POST"]
  },
  "log": {
    "access": {
      "enabled": true,
      "path": "log/access.log",
      "console": false
    },
    "error": {
      "debug": true,
      "enabled": true,
      "path": "log/error.log",
      "console": true
    },
    "warning": {
      "enabled": true
    }
  }
}
